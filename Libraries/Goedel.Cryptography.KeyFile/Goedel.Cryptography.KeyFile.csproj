<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(SolutionDir)CommonSettings.xml" />
  <PropertyGroup>
    <Product>Goedel.Cryptography.KeyFile</Product>
    <Description>Cryptographic key management.</Description>
